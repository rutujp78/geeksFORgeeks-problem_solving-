class Solution {
  public:
  
    // void solve(long long N, long long & ans) {
    //     if(N==0) return;
    //     ans+=(N*N*N);
    //     solve(N-1,ans);
    // }
    
  
    long long sumOfSeries(long long N) {
        // code here

        // Recursive Method
        // long long ans = 0;
        // solve(N,ans);
        // return ans;
        
        long long ans = 0;
        ans = N*(N+1)/2;
        return ans*ans;
    }
};
