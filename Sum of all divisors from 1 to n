class Solution
{
public:
    long long sumOfDivisors(int N)
    {
        // Write Your Code here
        long long sum = 0;
        
        // for(int i=1;i<=N;i++) {
        //     for(int j=1;j<=i;j++) {
        //         if(i%j==0) sum+=j;
        //     }
        // }
        
        for(int i=1;i<=N;i++) {
            sum+=(N/i)*i;
        }
        
        return sum;
    }
};
